config AdGuardHome 'AdGuardHome'
	option enabled '0'
	option httpport '3000'
	option redirect 'none'
	option configpath '/etc/AdGuardHome.yaml'
	option workdir '/tmp/AdGuardHome'
	option logfile ''
	option verbose '0'
	option binpath '/usr/bin/AdGuardHome'
	option upxflag ''

	option waitonboot '0'
